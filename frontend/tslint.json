{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rules": {
    "no-submodule-imports": [
      true,
      "redux-auth-wrapper",
      "history"
    ],
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-trailing-whitespace": false,
    "ordered-imports": false,
    "arrow-parens": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rulesDirectory": []
}
