{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rules": {
    "no-submodule-imports": [
      true,
      "redux-auth-wrapper",
      "history",
      "material-ui",
      "moment-timezone",
      "leaflet",
      "leaflet.markercluster",
      "react-dates",
      "redux-persist"
    ],
    "no-shadowed-variable": false,
    "no-implicit-dependencies": [
      [true, "dev"]
    ],
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "no-trailing-whitespace": true,
    "ordered-imports": false,
    "arrow-parens": true,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "prefer-const": true,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ]
  },
  "rulesDirectory": []
}
