{
  "name": "mvp-web",
  "version": "1.0.0",
  "description": "The most accurate and versatile meter data source for your metering applications",
  "main": "index.html",
  "repository": {
    "url": "git@dev6.elvaco.local:elvaco/mvp-web.git",
    "type": "git"
  },
  "author": "Must Tahir <must.tahir@invencon.se>",
  "license": "MIT",
  "scripts": {
    "clean": "node fuse clean",
    "start": "node fuse start && parcel serve src/index.html --port 4444",
    "serve": "node fuse serve && parcel serve src/index.html --port 4444",
    "dist": "node fuse dist && parcel build src/index.html --no-cache --no-source-maps && node fuse version",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "verify": "node fuse verify",
    "translations": "node fuse translations",
    "tslint": "./node_modules/tslint/bin/tslint --project tsconfig.json --config tslint.json --format codeFrame",
    "tsfix": "./node_modules/tslint/bin/tslint --project tsconfig.json --config tslint.json --format codeFrame --fix"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.test.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageDirectory": "coverage-reports",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(svg|png|jpg)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@types/chroma-js": "^1.3.7",
    "@types/classnames": "^2.2.6",
    "@types/i18next": "^8.4.3",
    "@types/jest": "^22.0.1",
    "@types/leaflet": "^1.2.11",
    "@types/lodash": "^4.14.116",
    "@types/material-ui": "0.20.7",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^9.3.0",
    "@types/react-dates": "^16.0.5",
    "@types/react-dom": "^16.0.8",
    "@types/react-leaflet": "^1.1.6",
    "@types/react-redux": "^5.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/react-virtualized": "^9.7.6",
    "@types/recharts": "^1.0.18",
    "@types/recompose": "^0.26.1",
    "@types/redux": "^3.6.0",
    "@types/redux-auth-wrapper": "^2.0.8",
    "@types/redux-mock-store": "^0.0.13",
    "@types/redux-thunk": "^2.1.0",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "^9.1.5",
    "axios-mock-adapter": "^1.15.0",
    "bottleneck": "^1.16.0",
    "cssnano": "^4.1.4",
    "fuse-box": "3.5.0",
    "fuse-box-typechecker": "^2.10.0",
    "graceful-fs": "^4.1.11",
    "i18next-conv": "^6.0.0",
    "i18next-scanner": "^2.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.6",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.10.1",
    "postcss": "^7.0.5",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.4.0",
    "request-promise": "^4.2.2",
    "seedrandom": "^2.4.4",
    "ts-jest": "^22.0.1",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chroma-js": "1.3.5",
    "classnames": "^2.2.6",
    "es6-shim": "^0.35.3",
    "history": "^4.6.3",
    "i18next": "^10.2.1",
    "i18next-xhr-backend": "^1.5.0",
    "leaflet": "^1.2.0",
    "leaflet.markercluster": "^1.2.0",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.2",
    "moment-timezone": "^0.5.16",
    "normalizr": "^3.2.4",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-dates": "^16.6.1",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.7.1",
    "react-leaflet-control": "^1.4.1",
    "react-leaflet-markercluster": "^1.1.8",
    "react-redux": "^5.0.7",
    "react-redux-typescript": "3.0.0-rc.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-virtualized": "^9.13.0",
    "recharts": "^0.22.4",
    "recompose": "^0.27.1",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.3",
    "redux-persist": "5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "uuid": "^3.3.2"
  }
}
