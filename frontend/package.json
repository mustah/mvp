{
  "name": "mvp-web",
  "version": "1.0.0",
  "description": "The most accurate and versatile meter data source for your metering applications",
  "main": "index.html",
  "repository": {
    "url": "git@dev6.elvaco.local:elvaco/mvp-web.git",
    "type": "git"
  },
  "author": "Must Tahir <must.tahir@invencon.se>",
  "license": "MIT",
  "scripts": {
    "clean": "node fuse clean",
    "start": "node fuse start && parcel serve src/index.html --port 4444 --log-level 1",
    "serve": "node fuse serve && parcel serve src/index.html --port 4444 --log-level 1",
    "dist": "node fuse dist && parcel build src/index.html --no-cache --no-source-maps --log-level 3 && node fuse version",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "type-check": "node fuse type-check",
    "verify": "node fuse verify",
    "translations": "node fuse translations",
    "tslint": "./node_modules/tslint/bin/tslint --project tsconfig.json --config tslint.json --format codeFrame",
    "tsfix": "./node_modules/tslint/bin/tslint --project tsconfig.json --config tslint.json --format codeFrame --fix"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    },
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.test.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "coverageDirectory": "coverage-reports",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(svg|png|jpg)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@types/chroma-js": "^1.4.1",
